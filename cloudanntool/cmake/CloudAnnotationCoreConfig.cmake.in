@PACKAGE_INIT@

# Find dependencies that are needed by consumers
find_package(OpenGL REQUIRED)
find_package(PCL REQUIRED COMPONENTS common io)

# Include the targets file (this also provides EnTT, spdlog, glfw, glad)
include("${CMAKE_CURRENT_LIST_DIR}/CloudAnnotationCoreTargets.cmake")

# Set variables for backwards compatibility
set(CloudAnnotationCore_LIBRARIES CloudAnnotationCore::cloudanncore)
set(CloudAnnotationCore_INCLUDE_DIRS "${PACKAGE_PREFIX_DIR}/include")

check_required_components(CloudAnnotationCore)
